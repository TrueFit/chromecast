{
  "name": "truefit_chromecast",
  "version": "1.0.0",
  "description": "Chromecast Display for Truefit",
  "readme": "../README.md",
  "repository": "https://github.com/TrueFit/chromecast.git",
  "scripts": {
    "start": "parallelshell \"npm run lint:tools\" \"npm run open:src\"",
    "open:src": "node tools/srcServer.js",
    "open:dist": "node tools/distServer.js",
    "lint:tools": "eslint webpack.config.js tools",
    "build:html": "node tools/buildHtml.js",
    "prebuild": "npm run build:html",
    "build": "node tools/build.js",
    "test": "cross-env NODE_ENV=test mocha --reporter progress --compilers js:babel/register --recursive \"./src/**/*.spec.js\"",
    "test:watch": "npm run test -- --watch"
  },
  "author": "Josh Gretz",
  "license": "MIT",
  "dependencies": {
    "boron": "0.2.3",
    "foundation-apps": "1.2.0",
    "object-assign": "4.0.1",
    "react": "0.14.7",
    "react-dom": "0.14.7",
    "react-promise": "1.1.1",
    "react-redux": "4.4.0",
    "react-router": "2.0.0",
    "redux": "3.3.1",
    "redux-form": "4.2.0",
    "redux-thunk": "1.0.3"
  },
  "devDependencies": {
    "babel": "6.5.2",
    "babel-eslint": "5.0.0",
    "babel-loader": "6.2.4",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-plugin-react-transform": "2.0.0",
    "browser-sync": "2.11.1",
    "chai": "3.5.0",
    "cheerio": "0.20.0",
    "colors": "1.1.2",
    "cross-env": "1.0.7",
    "css-loader": "0.23.1",
    "escope": "3.4.0",
    "eslint": "2.2.0",
    "eslint-loader": "1.3.0",
    "eslint-plugin-react": "4.1.0",
    "file-loader": "0.8.5",
    "mocha": "2.4.5",
    "node-sass": "3.4.2",
    "parallelshell": "2.0.0",
    "react-transform-catch-errors": "1.0.2",
    "react-transform-hmr": "1.0.2",
    "redbox-react": "1.2.2",
    "rimraf": "2.5.2",
    "sass-loader": "3.1.2",
    "style-loader": "0.13.0",
    "watch": "0.17.1",
    "webpack": "1.12.14",
    "webpack-dev-middleware": "1.5.1",
    "webpack-hot-middleware": "2.7.1",
    "yargs": "4.2.0"
  }
}
